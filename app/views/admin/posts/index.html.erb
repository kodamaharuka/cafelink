<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<% @posts.each do |post| %>
<div>
  <%= attachment_image_tag post, :image %>
  <p>投稿ユーザー画像：<%= image_tag('sample.jpg') %></p>
  <p>ショップ名：<%= post.cafe_name %></p>
  <p>説明：<%= post.introduction %></p>
  <p>ユーザーネーム：<%= post.user.name %></p>
</div>
<% end %>
<%= paginate @post_images %>