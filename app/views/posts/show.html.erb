<h1>Posts#show</h1>
<p>Find me in app/views/posts/show.html.erb</p>

<h1>画像投稿フォーム</h1>
<%= form_with model: @post, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>
  <h4>店名</h4>
  <%= f.text_field :cafe_name %>
  <h4>説明</h4>
  <%= f.text_area :introduction %>
  <%= f.submit '投稿' %>
<% end %>